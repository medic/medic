        <malnutrition_follow_up delimiter="#" id="malnutrition_follow_up" prefix="J1!malnutrition_follow_up!" version="2021-01-29_15-47">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <patient_id/>
              <name/>
              <parent>
                <_id/>
                <parent>
                  <_id/>
                  <name/>
                  <phone/>
                  <parent>
                    <_id/>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_name/>
          <patient_id/>
          <group_assessment>
            <available/>
            <followup_date/>
          </group_assessment>
          <summary tag="hidden">
            <summary_submit1/>
            <summary_submit2/>
          </summary>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </malnutrition_follow_up>
      </instance>

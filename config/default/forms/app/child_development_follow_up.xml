      <bind calculate="floor(decimal-date-time(today()) - decimal-date-time( /child_development_follow_up/inputs/contact/date_of_birth ) )" nodeset="/child_development_follow_up/patient_age_in_days" type="string"/>
      <bind calculate="../inputs/contact/name" nodeset="/child_development_follow_up/patient_name" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/child_development_follow_up/patient_id" type="string"/>
      <bind nodeset="/child_development_follow_up/group_assessment/go_to_clinic" required="true()" type="select1"/>
      <bind nodeset="/child_development_follow_up/group_assessment/agreed_to_go" required="true()" type="select1"/>
      <bind nodeset="/child_development_follow_up/group_assessment/date_planned" required="true()" type="date"/>
      <bind nodeset="/child_development_follow_up/summary/summary_submit1" readonly="true()" type="string"/>
      <bind nodeset="/child_development_follow_up/summary/summary_submit2" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/child_development_follow_up/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/child_development_follow_up/inputs">
      <label ref="jr:itext('/child_development_follow_up/inputs:label')"/>
      <group ref="/child_development_follow_up/inputs/contact">
        <label ref="jr:itext('/child_development_follow_up/inputs/contact:label')"/>
        <input ref="/child_development_follow_up/inputs/contact/_id">
          <label ref="jr:itext('/child_development_follow_up/inputs/contact/_id:label')"/>
          <hint ref="jr:itext('/child_development_follow_up/inputs/contact/_id:hint')"/>
        </input>
        <group ref="/child_development_follow_up/inputs/contact/parent">
          <label ref="jr:itext('/child_development_follow_up/inputs/contact/parent:label')"/>
          <group ref="/child_development_follow_up/inputs/contact/parent/parent">
            <label ref="jr:itext('/child_development_follow_up/inputs/contact/parent/parent:label')"/>
            <group ref="/child_development_follow_up/inputs/contact/parent/parent/parent">
              <label ref="jr:itext('/child_development_follow_up/inputs/contact/parent/parent/parent:label')"/>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group ref="/child_development_follow_up/group_assessment">
      <label ref="jr:itext('/child_development_follow_up/group_assessment:label')"/>
      <select1 ref="/child_development_follow_up/group_assessment/go_to_clinic">
        <label ref="jr:itext('/child_development_follow_up/group_assessment/go_to_clinic:label')"/>
        <item>
          <label ref="jr:itext('/child_development_follow_up/group_assessment/go_to_clinic/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/child_development_follow_up/group_assessment/go_to_clinic/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/child_development_follow_up/group_assessment/agreed_to_go">
        <label ref="jr:itext('/child_development_follow_up/group_assessment/agreed_to_go:label')"/>
        <item>
          <label ref="jr:itext('/child_development_follow_up/group_assessment/agreed_to_go/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/child_development_follow_up/group_assessment/agreed_to_go/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/child_development_follow_up/group_assessment/date_planned">
        <label ref="jr:itext('/child_development_follow_up/group_assessment/date_planned:label')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/child_development_follow_up/summary">
      <input appearance="h1 yellow" ref="/child_development_follow_up/summary/summary_submit1">
        <label ref="jr:itext('/child_development_follow_up/summary/summary_submit1:label')"/>
      </input>
      <input appearance="h2" ref="/child_development_follow_up/summary/summary_submit2">
        <label ref="jr:itext('/child_development_follow_up/summary/summary_submit2:label')"/>
      </input>
    </group>
    <group appearance="hidden" ref="/child_development_follow_up/data"/>
  </h:body>
</h:html>
